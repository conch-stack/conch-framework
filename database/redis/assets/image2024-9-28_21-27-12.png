    <script>
      try {
        window['UBT_API'].push({
          type: 'register',
          data: {
                // 替换为当前页的pageID
            pageId: '10650008581',
                // 替换为当前应用的appID
            appId: 700135
          }
        })
      } catch (e) {}
    </script>
    <script type="text/javascript" src="//webresource.c-ctrip.com/resaresonline/risk/ubtrms/latest/default/rms.js?v=20220524"></script>
</body>
</html>
